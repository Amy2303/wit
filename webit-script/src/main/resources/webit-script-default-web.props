


[webit.script.Engine]
textStatmentFactoryClass=webit.script.core.text.impl.ByteArrayTextStatmentFactory
resourceLoaderClass=webit.script.web.loaders.ServletLoader

vars=request,response

resolvers +=webit.script.web.resolvers.HttpSessionResolver,\
    webit.script.web.resolvers.HttpServletRequestResolver,\
    webit.script.web.resolvers.HttpServletRequestParametersResolver,\
    webit.script.web.resolvers.HttpServletRequestAttributesResolver,\
    webit.script.web.resolvers.HttpServletRequestHeaderResolver,\
    webit.script.web.resolvers.HttpServletRequestHeadersResolver



[webit.script.web.loaders.ServletLoader]

encoding=${DEFAULT_ENCODING}
root=/WEB-INF/templates

### servletContext will be setted by WebEngineUtil
## servletContext=


