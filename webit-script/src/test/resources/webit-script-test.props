
[engine]
resourceLoaderClass=webit.script.loaders.impl.LazyLoader-main
loggerClass=webit.script.loggers.impl.Slf4jLogger
vars=request,request2,session,session
initTemplates=/init.wit,/init2.wit,/init-junit-assert.wit


[webit.script.loaders.impl.LazyLoader-main]
loader=webit.script.loaders.impl.ClasspathLoader-test
timeout=100000


[loader]
root=webit/script/test/tmpls


[loader-error]
root=this/is/a/error/path


[webit.script.loaders.impl.ClasspathLoader-test]
@extends=loader-error,loader


[nativeSecurity]
list +='''
  org.junit.Assert
  java.lang.System.currentTimeMillis
  webit.script.test.util
'''


[global]
registers+=webit.script.global.TestGlobalRegister,webit.script.global.AssertGlobalRegister
