
[webit.script.Engine]
resourceLoaderClass=webit.script.loaders.impl.ClasspathLoader
loggerClass=webit.script.loggers.impl.Slf4jLogger
vars=request,request2,session,session
initTemplates=/init.wit,/init2.wit,/init-junit-assert.wit


[webit.script.loaders.impl.ClasspathLoader]
root=webit/script/test/tmpls


[webit.script.security.impl.DefaultNativeSecurityManager]
list +='''
  org.junit.Assert
  java.lang.System.currentTimeMillis
  webit.script.test.util
'''


[webit.script.global.DefaultGlobalManager]
registers+=webit.script.global.TestGlobalRegister,webit.script.global.AssertGlobalRegister
